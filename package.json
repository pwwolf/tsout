  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "ts-lint": "^4.5.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  }
}
