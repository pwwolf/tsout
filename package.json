{
  "name": "tsout",
  "version": "1.0.0",
  "description": "A TypeScript serialization library",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
